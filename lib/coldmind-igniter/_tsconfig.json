{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "noImplicitAny": false,
        "sourceMap": false,
        "strict": false,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "outDir": "dist",
        "baseUrl": "./",
        "paths": {
          "@Server/*": ["./Igniter.Core/*"],
          "@App/*": ["./TopZap.Backend/*"],
          "@Web/*": ["./Igniter.Web/*"],
          "@Tools/*": ["./Igniter.Tools/*"]
        }
    },
    "exclude": [
      "node_modules"
    ]
}
